<h1>Listing prayer_followups</h1>

<table>
  <tr>
    <th>Prayer</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @prayer_followups.each do |prayer_followup| %>
  <tr>
    <td><%= prayer_followup.prayer_id %></td>
    <td><%= prayer_followup.content %></td>
    <td><%= link_to 'Show', prayer_followup %></td>
    <td><%= link_to 'Edit', edit_prayer_followup_path(prayer_followup) %></td>
    <td><%= link_to 'Destroy', prayer_followup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Prayer followup', new_prayer_followup_path %>
