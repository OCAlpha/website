<h1>Listing charges</h1>

<table>
  <tr>
    <th>Office</th>
    <th>User</th>
    <th>Charge type</th>
    <th>Override value</th>
    <th>Reconciled</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @charges.each do |charge| %>
  <tr>
    <td><%= charge.office_id %></td>
    <td><%= charge.user_id %></td>
    <td><%= charge.charge_type_id %></td>
    <td><%= charge.override_value %></td>
    <td><%= charge.reconciled %></td>
    <td><%= link_to 'Show', charge %></td>
    <td><%= link_to 'Edit', edit_charge_path(charge) %></td>
    <td><%= link_to 'Destroy', charge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Charge', new_charge_path %>
